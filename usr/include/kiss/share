debug_filter() (
	[ "$1" != "--" ] || shift 1
	_level_0
	# printf '%s %s\n' '$scope' "$scope"
	# $func_name == debug_filter
	# printf '%s %s\n' '$func_name' "$func_name"
	# local scope="$1"
	# shift 1
	# local result=1

	[ -n "${DEBUG_AT_BACKGROUND+x}" ] && {
		(
			(
				exec 8>"$lock_log";
				flock -n 8 || {
					"$@" >> "$INIT_OUTPUT"
					interrupt
				}
				# exec 8>&- "$@" >> "$INIT_OUTPUT"
				"$@" >> "$INIT_OUTPUT"
				# exec 8>&-
			) &
		)
		return 0
	} ||
	[ -n "${KISS_DEBUG+x}" ] || return 0

	local ifs="$IFS"
	local IFS=$', |'
	# printf '%s %s\n' '$scope' "$scope"
	local item
	local match=0
	for item in ${KISS_DEBUG:-}; do
		case "$scope" in *"${item}"*)
			# result=0
			# match=1
			"$@" &
			break
		esac
	done
	IFS="$ifs"

	# equ "$match" "0" || "$@"
	# return "$result"
)

